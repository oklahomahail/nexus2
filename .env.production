# Production Environment Variables
# Copy to .env for local development or use in CI/CD

# API Configuration
VITE_API_ENVIRONMENT=production
VITE_API_BASE_URL=https://api.nexus.com/v1

# Claude AI API Key - Get from https://console.anthropic.com/
VITE_CLAUDE_API_KEY=your_production_claude_api_key_here

# Optional: Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DEBUG=false

# Optional: CDN Configuration
VITE_CDN_URL=https://cdn.nexus.com

# Optional: Sentry Configuration
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_SENTRY_ENVIRONMENT=production